{
  "version": 2,
  "buildCommand": "cd app && npm install && npm run build",
  "outputDirectory": "app/dist/app",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true
  }
} 